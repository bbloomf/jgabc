{
  "name": "GABC Transcription Tool",
  "version": "2.2.21",
  "manifest_version": 2,
  "description": "Tools for aiding GABC transcription",
  "icons": {
    "16": "icon/clear16.png",
    "32": "icon/clear32.png",
   "128": "icon/clear128.png"
  },
  "app": {
    "launch": {
      "local_path": "transcriber.html"
    }
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://illuminarepublications.com 'unsafe-eval'; object-src 'self'"
}
